{
    "logStatementNoArguments": {
        "py": "print()",
        "js": "console.log()",
        "gast": {
            "type": "root",
            "body": [
                {
                    "type": "logStatement",
                    "args": []
                }
            ]   
        }
    },
    "logStatementWithArguments": {
        "py": "print('hello world')",
        "js": "console.log('hello world')",
        "gast": {
            "type": "root",
            "body": [
                {
                    "type": "logStatement",
                    "args": ["hello world"]
                }
            ]   
        }
    },
    "customStatementNoArgs": {
        "py": "custom_function_name()",
        "js": "customFunctionName()",
        "gast": {
            "type": "root",
            "body": [
                {"type": "customStatement"}
            ]
        }
    }
}